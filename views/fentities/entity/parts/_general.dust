<div class="entity" data-subject="{subject}">
	{!<p><small>{id}</small></p>!}
	<h2>{#get key="rdfs:label"}
			{#.}
				{#getValue key="@value"/}
			{/.}
		{/get}
	</h2>

	<p>{#get key="rdfs:comment"}
		{#.}
			{#getValue key="@value"/}
		{/.}
	{/get}</p>

	<p>ID:{#getValue key="@id"/}</p>

	<p>{#get key="@type"}
		{.}<br/>
	{/get}</p>

	{?attributes}
		<ul style="display:none">
		{#attributes}
		   <li>{attr} : {value}</li>
		{/attributes}
	  </ul>
	{/attributes}
</div>