<div class="entity">
	{!<p><small>{id}</small></p>!}
	<h2>{#get key="rdfs:label"}
			{#.}
				{#getValue key="@value"/}
			{/.}
		{/get}
	</h2>

	{! <p>{#getValue key="rdfs:comment"}
	{#.}
		{#getValue key="@value"/}
	{/.}
	{/getValueList}</p> !}
	{!#getValueList key="@type"}
		<p>{.}</p>
	{/getValueList!}
	{?attributes}
		<ul style="display:none">
		{#attributes}
		   <li>{attr} : {value}</li>
		{/attributes}
	  </ul>
	{/attributes}
</div>