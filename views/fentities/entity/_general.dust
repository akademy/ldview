<div class="entity">
	{!<p><small>{id}</small></p>!}
	<h2>{#get key="rdfs:label"}
			{#.}
				{#getValue key="@value"/}
			{/.}
		{/get}
	</h2>
	<p>{#get key="@id"}
		ID: {.}
	{/get}
	</p>
	<p>{#get key="rdfs:comment"}
	{#.}
		Comment: {#getValue key="@value"/}
	{/.}
	{/get}</p>
	{#get key="@type"}
		<p>{.}</p>
	{/get}
	{?attributes}
		<ul style="display:none">
		{#attributes}
		   <li>{attr} : {value}</li>
		{/attributes}
	  </ul>
	{/attributes}
</div>