<div class="entity">
	{!<p><small>{id}</small></p>!}
	<h2>{#getValueList key="rdfs:label"}
		{#.}
			{#getValue key="@value"/}
		{/.}
	{/getValueList}</h2>
	<p>{#getValueList key="rdfs:comment"}
	{#.}
		{#getValue key="@value"/}
	{/.}
	{/getValueList}</p>
	{!#getValueList key="@type"}
		<p>{.}</p>
	{/getValueList!}
	{?attributes}
		<ul style="display:none">
		{#attributes}
		   <li>{attr} : {value}</li>
		{/attributes}
	  </ul>
	{/attributes}
</div>